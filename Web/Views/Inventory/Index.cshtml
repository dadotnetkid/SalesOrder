<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body" id="inventory-grid-container">

            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        load();
    })

    function load() {
        $.get('@Url.Action("InventoryPartial")', {}, function (xhr) {
            $('#inventory-grid-container').html(xhr);
        })
    }
</script>