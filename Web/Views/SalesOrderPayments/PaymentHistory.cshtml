<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header"><h5>Payment of Payment History</h5></div>
            <div class="card-body" id="payment-history-container">

            </div>
        </div>

    </div>
</div>
<script>
    $(document).ready(function() {
        load();
    });

    function load() {
        showLoading();
        $.get('@Url.Action("PaymentHistoryPartial")', function (xhr) {
            $('#payment-history-container').html(xhr);
            hideLoading();
        });
    }

</script>
